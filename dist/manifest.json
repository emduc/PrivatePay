{
  "manifest_version": 3,
  "name": "Mini ETH Wallet",
  "version": "0.0.1",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": ["<all_urls>"]
}